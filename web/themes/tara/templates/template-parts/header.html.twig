<!-- Start: Header -->
<header id="header">
  {% if page.header_top or all_icons_show %}
    {% include '@tara/template-parts/header_top.html.twig' %}
  {% endif %}
  <div class="header">
     <div class="" style="padding:0 80px;">
      <div class="container-fluid pl-0">
        {% if page.site_branding %}
         <div class="row">
        <div class="col-md-6">
            <img style="height: 110px;" src="{{ base_path ~ directory }}/images/Main_NBCGDC.png">
         </div>
         

<div class="col-md-6">
          <div class="d-flex p-2" style="float:right;">  
    <div class="p-1">  <img  style="height:75px;" src="{{ base_path ~ directory }}/images/logo-amrit.jpg"></div>
    <div class="p-1">   <img  style="height:75px;" src="{{ base_path ~ directory }}/images/G20_India_2023.png"></div>
    <div class="p-1"> <img  style="height:75px;" src="{{ base_path ~ directory }}/images/logo2.png"></div>
  </div>
</div>
         </div>
         </div>
          	
          <!--/.site-branding -->
        {% endif %} <!--/.end if for site_branding -->
        {% if page.search_box or  page.primary_menu %}
        </div>
          <div class="header-right justify-content-center">
            <!-- Start: primary menu region -->
            {% if page.primary_menu %}
            <div class="mobile-menu">
              <i class="fa fa-bars" aria-hidden="true"></i>
            </div><!-- /mobile-menu -->
            <div class="primary-menu-wrapper">
              <div class="menu-wrap">
                <div class="close-mobile-menu">X</div>
                {{ page.primary_menu }}
              </div>
            </div><!-- /primary-menu-wrapper -->
            {% endif %}<!-- end if for page.primary_menu -->
            {% if page.search_box %}
             
              <!--/.full-page-search -->
            {% endif %} <!-- end if for page.search_box -->
          </div> <!--/.header-right -->
        {% endif %}<!-- end if for page.search_box or  page.primary_menu -->
      </div> <!--/.header-container -->
    </div> <!--/.container -->
  </div><!-- /.header -->
</header>
<!-- End: Header -->
