{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/autologout": "^1.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/block_class": "^2.0@beta",
        "drupal/captcha": "^1.2",
        "drupal/clientside_validation": "^3.0@RC",
        "drupal/colorbox": "^1.10",
        "drupal/components": "^2.4",
        "drupal/computed_field": "^3.0@alpha",
        "drupal/computed_field_plugin": "^1.3",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_update": "^1.7",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/counter": "^1.2",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/custom_search": "^1.0@beta",
        "drupal/date_popup": "^1.1",
        "drupal/devel": "^4.1",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity": "^1.3",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_print": "^2.5",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/entityreference_extensions": "^1.2",
        "drupal/extlink": "^1.6",
        "drupal/features": "^3.12",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_formatter_file_size": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/field_validation": "^1.0@beta",
        "drupal/file_upload_secure_validator": "^1.4",
        "drupal/filemime": "^1.9",
        "drupal/flexslider": "^2.0",
        "drupal/flipbook": "^1.2",
        "drupal/fontawesome": "^2.23",
        "drupal/fontawesome_menu_icons": "^1.9",
        "drupal/gdoc_field": "^1.3",
        "drupal/genpass": "^1.1",
        "drupal/google_analytics": "^4.0",
        "drupal/google_cse": "^3.5",
        "drupal/group": "^1.4",
        "drupal/gtranslate": "^1.14",
        "drupal/icons": "^2.0@alpha",
        "drupal/ip": "^3.1",
        "drupal/job_scheduler": "^3.1",
        "drupal/jquery_ui_checkboxradio": "^1.2",
        "drupal/lang_dropdown": "^2.0",
        "drupal/languagefield": "^1.8",
        "drupal/languages_dropdown": "^1.2",
        "drupal/libraries": "^3.0@beta",
        "drupal/login_destination": "^2.0@beta",
        "drupal/login_security": "^2.0",
        "drupal/magazine_lite": "^1.3",
        "drupal/memcache": "^2.5",
        "drupal/memcache_storage": "^2.0",
        "drupal/menu_block": "^1.8",
        "drupal/menu_item_extras": "^2.16",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_manipulator": "^3.0",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.19",
        "drupal/mfd": "^2.0",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panelizer": "^4.4",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.14",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.0",
        "drupal/password_policy": "^3.1",
        "drupal/pathauto": "^1.10",
        "drupal/php": "^1.1",
        "drupal/profile": "^1.3",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/redirect": "^1.8",
        "drupal/rename_admin_paths": "^2.1",
        "drupal/search_api": "^1.23",
        "drupal/seckit": "^2.0",
        "drupal/securimage": "^9.0",
        "drupal/security_review": "^1.0@alpha",
        "drupal/simple_pass_reset": "^1.1",
        "drupal/sitemap": "^2.0@beta",
        "drupal/svg_image": "^1.16",
        "drupal/tamper": "^1.0@alpha",
        "drupal/taxonomy_import": "^2.0",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/token": "^1.10",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_vardumper": "^3",
        "drupal/updated": "^1.1",
        "drupal/user_registrationpassword": "^2.0@alpha",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_aggregator": "^2.0",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_conditional": "^1.4",
        "drupal/views_data_export": "^1.1",
        "drupal/views_dates": "^1.1",
        "drupal/views_field_formatter": "^1.13",
        "drupal/views_slideshow": "^4.8",
        "drupal/views_templates": "^1.1",
        "drupal/visitors": "^2.7",
        "drupal/we_megamenu": "^1.13",
        "drupal/webform": "^6.1",
        "drupal/webform_validation": "^2.0@alpha",
        "drupal/workbench": "^1.3",
        "drupal/workbench_moderation": "^1.6",
        "drupal/xmlsitemap": "^1.2",
        "drupal/xss_prevention": "^1.1",
        "drupal/zen": "^7.0@alpha",
        "league/commonmark": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "robloach/component-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drush/drush": "^11.0"
    }
}
